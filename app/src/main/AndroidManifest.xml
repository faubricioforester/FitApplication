<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ic.fitapptec.com.fitapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".CommunityActivity" />
        <activity android:name=".SavedActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".MyTrainerActivity" />
        <activity android:name=".StatisticActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".CommunityWallActivity" />
        <activity android:name=".NewPostActivity" />
        <activity android:name=".PostCommentsActivity" />
        <activity android:name=".MyWorkoutRoutinesActivity" />
        <activity android:name=".RoutineExercisesActivity" />
        <activity android:name=".ExerciseDetailActivity"></activity>
    </application>

</manifest>